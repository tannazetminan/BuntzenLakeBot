# Buntzen Lake Parking Bot Configuration

# User credentials
PHONE_NUMBER=your_phone_number_here
VERIFICATION_CODE= - Leave empty, OTP will be entered manually when received

# Vehicle information
LICENSE_PLATE=your_license_plate_here
PROVINCE=BC
VEHICLE_COLOR=BLACK
VEHICLE_MAKE=Tesla
VEHICLE_MODEL=Y

# Booking preferences
PASS_TYPE=all_day
# PASS_TYPE=half_day

# Date preferences (format: DD)
PREFERRED_DATE=17
# Leave empty for next available date

# Bot settings
HEADLESS=false
# Set to true for production (no browser UI)
DELAY_BETWEEN_ACTIONS=2000
LONG_WAIT_TIME=500
# Milliseconds between actions to avoid detection

# Logging
LOG_LEVEL=info
# Options: error, warn, info, debug

# Retry settings
MAX_RETRIES=3
RETRY_DELAY=5000
